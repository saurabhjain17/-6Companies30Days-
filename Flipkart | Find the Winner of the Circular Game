class Solution:
    def findTheWinner(self, n: int, k: int) -> int:
        arr=[i for i in range(1,n+1)]
        i=0
        while len(arr)>1:
            i=(i+k-1)%len(arr)
            
            m=arr.pop(i)
            if i==len(arr):
                   i=0
            
        return arr[0]  
