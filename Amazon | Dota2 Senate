class Solution:
    def predictPartyVictory(self, senate: str) -> str:
        l = len(senate)
        R = [i for i in range(l) if senate[i] == 'R']
        D = [i for i in range(l) if senate[i] == 'D']
        
        while len(D) and len(R):
            if D[0] > R[0]:
                R.append(R[0] + l)
            else:
                D.append(D[0] + l)
            del D[0]
            del R[0]
                
        return "Dire" if len(D) != 0 else "Radiant"
